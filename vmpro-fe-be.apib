                    "id": "http://jsonschema.net/defaultQuality",
                    "required": false
                },
                "responsive": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/responsive",
                    "required": false
                },
                "useHtmlFirst": {
                    "type": "boolean",
                    "id": "http://jsonschema.net/useHtmlFirst",
                    "required": false
                }
            }
            }
+ Response 200


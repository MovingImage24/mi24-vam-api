    + Body
    
            {
                "name": "name of new player-definition",
                "playerSkinId": "uuid1"
            }
            
    + Schema
    
            {
                "type": "object",
                "$schema": "http://json-schema.org/draft-03/schema",
                "id": "http://jsonschema.net",
                "required": false,
                "properties": {
                    "name": {
                        "type": "string",
                        "id": "http://jsonschema.net/name",
                        "required": true
                    },
                    "playerSkinId": {
                        "type": "string",
                        "id": "http://jsonschema.net/playerSkinId",
                        "required": true
                    }
                }
            }
+ Response 201
    + Header
    
            Location: /vam/rest/vms/{videoManagerId}/player-definitions/4

